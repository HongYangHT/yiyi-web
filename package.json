    "less-loader": "^5.0.0",
    "lint-staged": "^10.2.8",
    "mini-css-extract-plugin": "^0.9.0",
    "mockjs": "^1.1.0",
    "nodemon": "^2.0.3",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "ora": "^4.0.3",
    "portfinder": "^1.0.25",
