    "node": ">= 8.12.0",
    "npm": ">= 3.0.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "@babel/runtime": "^7.7.6",
    "@babel/runtime-corejs2": "^7.7.6",
    "axios": "^0.19.2",
    "element-ui": "^2.13.0",
    "qs": "^6.9.4",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.3",
    "vuex-persistedstate": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "lint-staged": {
    "src/**/*.{js,ts,vue}": [
      "npm run lint",
      "git add"
    ],
    "src/**/*.{html,vue,css,sass,scss}": [
      "npm run stylelint",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
