{
  "name": "movee-template",
  "version": "1.0.0",
  "description": "vue template tree-shaking code split",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --ext .js,.vue,.ts,.tsx --fix src",
    "stylelint": "stylelint --cache src/**/*.{html,vue,css,sass,scss,less,stylus} --fix",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --color --progress --open 'google chrome' --config build/webpack.dev.js",
    "start": "nodemon --watch build/ --exec  npm run dev",
    "analyzer": "cross-env NODE_ENV=production webpack --config build/webpack.analyzer.js",
    "build": "cross-env NODE_ENV=production webpack --color --progress --config build/webpack.prod.js",
    "build:tip": "node ./build/build.js",
    "build:prod": "npm-run-all build build:tip",
    "openServer": "http-server dist/ -o -g -p 8008",
    "server": "npm-run-all build openServer",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "changeVersion": "npm version patch --no-git-tag-version",
    "precommit": "lint-staged",
    "build:types": "tsc"
  },
  "author": "movee-frontend",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
